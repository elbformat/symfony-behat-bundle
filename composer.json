{
  "name": "elbformat/symfony-behat-bundle",
  "description": "Predefined, extensible behat contexts for symfony applications.",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony",
    "behat"
  ],
  "authors": [
    {
      "name": "Hannes Giesenow",
      "email": "hannes.giesenow@elbformat.de",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Elbformat\\SymfonyBehatBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-json": "*",
    "behat/behat": "^3.8",
    "friends-of-behat/symfony-extension": "^2.2",
    "symfony/dom-crawler": "^5.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.11.0",
    "phpunit/phpunit": "8.5.30",
    "vimeo/psalm": "4.27.0",
    "monolog/monolog": "2.8.0",
    "symfony/framework-bundle": "^5.4",
    "doctrine/orm": "2.13.2",
    "symfony/swiftmailer-bundle": "^3.5"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
